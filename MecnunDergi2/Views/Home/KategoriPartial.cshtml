@model IEnumerable<MecnunDergi2.Models.Kategori>
@{
    Layout = null;
}
<h4>Kategoriler</h4>
<div class="row">
    <div class="col-lg-6">
        @foreach (var item in Model)
        {
            <ul class="list-unstyled">
                <li>
                    <a href="#">@item.KategoriAdı</a>
                </li>
            </ul>
        }
    </div>
</div>
